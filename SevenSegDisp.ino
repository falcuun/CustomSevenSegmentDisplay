#define FIRST 2
#define SECOND 3
#define THIRD 4
#define FOURTH 5
#define FIFTH 6
#define SIXTH 7
#define SEVENTH 8


void setup()
{
  pinMode(FIRST, OUTPUT);
  pinMode(SECOND, OUTPUT);
  pinMode(THIRD, OUTPUT);
  pinMode(FOURTH, OUTPUT);
  pinMode(FIFTH, OUTPUT);
  pinMode(SIXTH, OUTPUT);
  pinMode(SEVENTH, OUTPUT);

  Serial.begin(9600);
}

void loop()
{}
